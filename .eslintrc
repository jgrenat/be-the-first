{
	"env": {
		"browser": true,
		"es6": true
	},
	"ecmaFeatures": {
		"modules": true
	},
	"rules": {
		"no-var": 2,
		"comma-dangle": 2,
		"valid-jsdoc": [2, { "requireParamDescription": false }],
		"block-scoped-var": 2,
		"dot-notation": [2, {"allowKeywords": false}],
		"no-else-return": 2,
		"no-multi-spaces": 0,
		"radix": 2,
		"wrap-iife": 2,
		"global-strict": 0,
		"strict": [2, "global"],
		"indent": [2, "tab"],
		"quotes": [2, "single"],
		"key-spacing": 0,
		"semi": [2, "always"],
		"no-use-before-define": [2, "nofunc"],
		"no-new": 0,
		"no-unused-expressions": 0
	}
}
